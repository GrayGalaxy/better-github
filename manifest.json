{
	"name": "__MSG_name__",
	"version": "0.2.4",
	"manifest_version": 3,
	"description": "__MSG_description__",
	"icons": { "48": "assets/icons/48L.png" },
	"default_locale": "en",
	"homepage_url": "https://github.com/",
	"options_ui": { "page": "src/options.html" },
	"content_scripts": [
		{
			"matches": ["https://github.com/*"],
			"css": ["dist/inject.min.css"],
			"js": ["dist/jquery.min.js", "src/inject.js"],
			"run_at": "document_end"
		}
	],
	"permissions": ["storage"]
}
